<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.organ.model.TextCode" table="t_textcode" catalog="organ">
        <id name="id" type="java.lang.Integer">
            <column name="id" />
            <generator class="identity" />
        </id>
        <property name="phoneNum" type="java.lang.String">
            <column name="phone_number" not-null="true" />
        </property>
        <property name="textCode" type="java.lang.String">
            <column name="text_code" not-null="true" />
        </property>
        <property name="createTime" type="java.lang.Long">
            <column name="create_time" not-null="true" />
        </property>
    </class>
</hibernate-mapping>
